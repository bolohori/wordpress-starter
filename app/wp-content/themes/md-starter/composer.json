{
    "name": "wearemd/md-starter",
    "description": "",
    "require": {
        "timber/timber": "^1.9.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "wp-coding-standards/wpcs": "2.3.0"
    },
    "autoload": {
        "psr-4": {
            "Theme\\": "lib/",
            "Theme\\Actions\\": "lib/actions",
            "Theme\\Filters\\": "lib/filters"
        }
    },
    "scripts": {
        "config-set": "vendor/bin/phpcs --config-set installed_paths wpcs",
        "lint": "vendor/bin/phpcs --standard=WordPress"
    }
}
